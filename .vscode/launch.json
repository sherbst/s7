  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug encode",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/target/debug/s7",
      "args": ["encode", "input/input.png", "output/debug.s7"],
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Debug decode",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceRoot}/target/debug/s7",
      "args": ["decode", "output/debug.s7", "output/debug.png"],
      "cwd": "${workspaceRoot}"
    }
  ]
}
